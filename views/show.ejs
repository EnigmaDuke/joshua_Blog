<%- include ("partials/header.ejs")  %>

<h1> <%= post.title %> </h1>

<div id="container">
  <div id="show_container"> 
    <img src=" <%= post.image %> " alt="">
    <p> <%= post.body %> </p>
    <hr />

    <p>Tagged with: 
      <% for(let i = 0; i <post.tags.length; i++){ %>
        <a href=""> <%= "#" + post.tags[i] %> </a>
      <% } %>
    </p>

  </div>
</div>


<%- include ("partials/footer.ejs")  %>